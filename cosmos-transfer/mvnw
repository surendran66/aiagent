#!/bin/sh

MAVEN_WRAPPER_DIR=$(dirname "$0")/.mvn/wrapper
MAVEN="mvn"
if [ -x "$MAVEN_WRAPPER_DIR/maven-wrapper.jar" ]; then
  JAVA_VERSION=$(java -version 2>&1 | awk -F[\"_] 'NR==1{print $2}')
  JAVA_MAJOR=$(echo $JAVA_VERSION | cut -d. -f1)
  if [ $JAVA_MAJOR -ge 17 ]; then
    exec java -jar "$MAVEN_WRAPPER_DIR/maven-wrapper.jar" "$@"
  fi
fi
exec mvn "$@"
